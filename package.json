{
	"name": "bot",
	"description": "The ultimate AI-powered Discord bot",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "swc --delete-dir-on-start src --out-dir build",
		"dev_rest": "nodemon --watch src/rest/ --exec \"npm run build && npm run rest\"",
		"dev_gateway": "nodemon --watch src/gateway/ --exec \"npm run build && npm run gateway\"",
		"dev_db": "nodemon --watch src/db/ --exec \"npm run build && npm run db\"",
		"dev_bot": "nodemon --watch src/bot/ --exec \"npm run build && npm run bot\"",
		"rest": "node build/rest/mod.js",
		"gateway": "node build/gateway/mod.js",
		"db": "node build/db/mod.js",
		"bot": "node build/bot/mod.js",
		"lint": "npx eslint --fix src/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ampere-ai/bot.git"
	},
	"author": "f1nniboy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ampere-ai/bot/issues"
	},
	"homepage": "https://github.com/ampere-ai/bot#readme",
	"dependencies": {
		"@discordeno/bot": "^19.0.0-next.b2f65c2",
		"@discordeno/gateway": "^19.0.0-next.b2f65c2",
		"@discordeno/rest": "^19.0.0-next.b2f65c2",
		"@discordeno/types": "^19.0.0-next.b2f65c2",
		"@discordeno/utils": "^19.0.0-next.b2f65c2",
		"@dqbd/tiktoken": "^1.0.7",
		"@napi-rs/canvas": "^0.1.44",
		"@supabase/supabase-js": "^2.32.0",
		"@types/emoji-js": "^3.5.0",
		"@waylaidwanderer/fetch-event-source": "^3.0.1",
		"colorette": "^2.0.20",
		"dayjs": "^1.11.9",
		"dotenv": "^16.0.3",
		"emoji-js": "^3.7.0",
		"express": "^4.18.2",
		"express-form-data": "^2.0.22",
		"node-fetch": "^3.2.10",
		"rabbitmq-client": "^4.2.1",
		"redis": "^4.6.7",
		"swc": "^1.0.11"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/express-form-data": "^2.0.2",
		"@types/node": "^17.0.23",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"eslint": "^8.47.0",
		"nodemon": "^2.0.15"
	}
}
